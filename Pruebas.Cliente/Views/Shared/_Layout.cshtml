                            <div class="sb-nav-link-icon"><i class="fas fa-columns"></i></div>
                            Productos
                            <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                        </a>

                        <div class="collapse" id="collapseProd" aria-labelledby="headingOne" data-bs-parent="#sidenavAccordion">
                            <nav class="sb-sidenav-menu-nested nav">
                                <a class="nav-link" asp-area="" asp-controller="Producto" asp-action="ProductoIndex">Lista Productos</a>
                                <a class="nav-link" asp-area="" asp-controller="Producto" asp-action="AgregarProducto">Agregar Producto</a>
                                <a class="nav-link" asp-area="" asp-controller="Producto" asp-action="ActualizarProducto">Actualizar Producto</a>
                                <a class="nav-link" asp-area="" asp-controller="Producto" asp-action="EliminarProducto">Eliminar Producto</a>
