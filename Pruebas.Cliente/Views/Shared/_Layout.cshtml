                            <div class="sb-nav-link-icon"><i class="fas fa-columns"></i></div>
                            Usuarios
                            <div class="sb-sidenav-collapse-arrow"><i class="fas fa-angle-down"></i></div>
                        </a>

                        <div class="collapse" id="collapseUsr" aria-labelledby="headingOne" data-bs-parent="#sidenavAccordion">
                            <nav class="sb-sidenav-menu-nested nav">
                                <a class="nav-link" asp-area="" asp-controller="Usuario" asp-action="Index">Lista Usuarios</a>
                               @*  <a class="nav-link" asp-area="" asp-controller="Usuario" asp-action="AgregarUsuario">Agregar Usuarios</a>
                                <a class="nav-link" asp-area="" asp-controller="Usuario" asp-action="EditarUsuario">Actualizar Usuarios</a>
                                <a class="nav-link" asp-area="" asp-controller="Usuario" asp-action="EliminarUsuario">Eliminar Usuarios</a> *@
